<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="224.0" prefWidth="290.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.ruslan.dblb2.forms.controllers.UserCUDialog">
   <Pane prefHeight="224.0" prefWidth="290.0">
      <Label layoutX="14.0" layoutY="18.0" text="First name:"/>
      <TextField fx:id="firstNameText" layoutX="117.0" layoutY="12.0"/>

      <Label layoutX="14.0" layoutY="49.0" text="Last name:"/>
      <TextField fx:id="lastNameText" layoutX="117.0" layoutY="43.0"/>

      <Label layoutX="14.0" layoutY="80.0" text="Email: "/>
      <TextField fx:id="emailText" layoutX="117.0" layoutY="74.0"/>

      <Label layoutX="14.0" layoutY="113.0" text="Password:"/>
      <TextField fx:id="passwordText" layoutX="117.0" layoutY="107.0"/>

      <TextField fx:id="phoneText" layoutX="117.0" layoutY="140.0"/>
      <Label layoutX="14.0" layoutY="146.0" text="Phone number:"/>

      <Button layoutX="15.0" layoutY="185.0" mnemonicParsing="false" onAction="#close" prefHeight="26.0"
              prefWidth="128.0" text="Cancel"/>
      <Button layoutX="150.0" layoutY="185.0" mnemonicParsing="false" onAction="#save" prefHeight="26.0"
              prefWidth="128.0" text="Save"/>
   </Pane>
</AnchorPane>
